{
    "manifest_version": 2,
    "name": "Emplenar llocs",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["https://appweb.edu.gva.es/*"],
        "js": ["content_script.js"]
    }],
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "48": "/icon.png"
        },
        "default_title": "Emplenar llocs",
        "default_popup": "popup.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "addon@chverma.com",
            "strict_min_version": "57.0"
        }
    },
    "permissions": [
        "https://appweb.edu.gva.es/*",
        "http://decasaalcole.cartodb.com/*",
        "activeTab",
        "tabs",
        "storage"
    ]
}